    "@types/react-helmet": "5.0.8",
    "@types/react-native": "0.57.38",
    "@types/recompose": "0.30.5",
    "babel-plugin-react-native-web": "0.11.5",
    "prettier": "1.18.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3"
