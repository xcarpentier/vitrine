{
  "name": "@vitrine/web",
  "private": true,
  "version": "0.0.1",
  "description": "vitrine: professional website",
  "scripts": {
    "cleaning": "rm -rf ./dist/ && rm -rf ./public/ && rm -rf .cache/ rm -rf node_modules",
    "compile": "tsc --noEmit",
    "build": "gatsby build",
    "deploy": "yarn clean && yarn compile && yarn build && gh-pages -d public",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo 'TODO: test'"
  },
  "dependencies": {
    "@vitrine/common": "0.0.1",
    "gatsby": "2.13.12",
    "gatsby-image": "^2.0.30",
    "gatsby-plugin-google-analytics": "2.1.1",
    "gatsby-plugin-manifest": "2.2.1",
    "gatsby-plugin-offline": "2.2.1",
    "gatsby-plugin-react-helmet": "3.1.0",
    "gatsby-plugin-react-native-web": "2.0.0-beta.0",
    "gatsby-plugin-sharp": "2.2.3",
    "gatsby-plugin-typescript": "2.1.0",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-filesystem": "2.1.3",
    "gatsby-transformer-sharp": "2.2.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
